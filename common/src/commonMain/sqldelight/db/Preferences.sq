CREATE TABLE preferences (
    id INTEGER NOT NULL PRIMARY KEY DEFAULT 0,
    nativeLanguage TEXT NOT NULL,
    nativeName TEXT NOT NULL,
    targetLanguage TEXT NOT NULL,
    targetName TEXT NOT NULL
);

select:
SELECT nativeLanguage, nativeName, targetLanguage, targetName FROM preferences;

set:
REPLACE INTO preferences (id, nativeLanguage, nativeName, targetLanguage, targetName) VALUES (0, ?, ?, ?, ?);